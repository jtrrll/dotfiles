{
  "add error context": {
    "body": [
      "builtins.addErrorContext ${1:context} ${0:return}"
    ],
    "description": "Adds error context to an expression.",
    "prefix": "error"
  },
  "assert condition": {
    "body": [
      "assert ${1:condition}; $0"
    ],
    "description": "Asserts a condition is true.",
    "prefix": "assert"
  },
  "nixpkgs overlay": {
    "body": [
      "final: prev: {",
      "\t$0",
      "}"
    ],
    "description": "Defines a Nixpkgs overlay.",
    "prefix": "overlay"
  },
  "trace": {
    "body": [
      "builtins.trace ${1:output} ${0:return}"
    ],
    "description": "Prints an expression.",
    "prefix": "trace"
  }
}
